<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<yogesh.atArxxus.newsreader.Preview android:id="@+id/preview"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
	</yogesh.atArxxus.newsreader.Preview>

	<yogesh.atArxxus.newsreader.TouchView android:id="@+id/left_top_view"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
	</yogesh.atArxxus.newsreader.TouchView>

	<Button
	    android:id="@+id/startcamerapreview"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentBottom="true"
	    android:layout_alignParentRight="true"
	    android:layout_marginRight="114dp"
	    android:background="@drawable/camera_button_pressed" />

</RelativeLayout>
